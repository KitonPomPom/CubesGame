        tools:layout="@layout/fragment_playing_with_pictures" />
    <fragment
        android:id="@+id/CameraFragment"
        android:name="io.ak1.pix.PixFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera" />
    <fragment
        android:id="@+id/fragmentImageList"
        android:name="kitonpompom.cubesgame.activities.FragmentImageList"
        android:label="fragment_image_list"
        tools:layout="@layout/fragment_image_list" />
    <fragment
        android:id="@+id/fragmentPlayingWithPicturesTwo"
        android:name="kitonpompom.cubesgame.activities.FragmentPlayingWithPicturesTwo"
        android:label="FragmentPlayingWithPicturesTwo" />
    <fragment
        android:id="@+id/fragmentPlayingWithPicturesMedium"
        android:name="kitonpompom.cubesgame.activities.FragmentPlayingWithPicturesMedium"
        android:label="fragment_playing_with_pictures_medium"
        tools:layout="@layout/fragment_playing_with_pictures_medium" />
    <fragment
        android:id="@+id/fragmentPlayingWithPicturesEasy"
        android:name="kitonpompom.cubesgame.activities.FragmentPlayingWithPicturesEasy"
